<script>
  $(document).ready(function() {
    $('#with_to_account').typeahead({
      name: 'to_account_name',
      local: <%= raw(@with_to_accounts.map {|cust| cust.name }.to_json(:only => [:name ])) %>
    });
    $('#with_from_account').typeahead({
      name: 'from_account_name',
      local: <%= raw(@with_from_accounts.map {|cust| cust.name }.to_json(:only => [:name ])) %>
    });
    $('#dep_to_account').typeahead({
      name: 'dep_to_account_name',
      local: <%= raw(@dep_to_accounts.map {|cust| cust.name }.to_json(:only => [:name ])) %>
    });
    $('#dep_from_account').typeahead({
      name: 'dep_from_account_name',
      local: <%= raw(@dep_from_accounts.map {|cust| cust.name }.to_json(:only => [:name ])) %>
    });
    $('#tran_to_account').typeahead({
      name: 'transferred_to_name',
      local: <%= raw(@tran_to_accounts.map {|cust| cust.name }.to_json(:only => [:name ])) %>
    });
    $('#tran_from_account').typeahead({
      name: 'transferred_from_name',
      local: <%= raw(@tran_from_accounts.map {|cust| cust.name }.to_json(:only => [:name ])) %>
    });
  });
</script>


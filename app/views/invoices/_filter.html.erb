      <tbody>
        <tr>
          <td><label>Invoice no</label></td>
          <td id="invoiceno"></td>
          <td ><label><%= @company.label.customer_label%></label></td>
          <td id="customer" data-customer="<%= Account.company_contacts(@company.id) %>"></td>
           <td><label>Create Date</label></td>
          <td id="createDate"></td>
        
        </tr>
        <tr>
          <td><label>Status</label></td>

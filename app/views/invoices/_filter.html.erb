          <td id="project" class='purchase_project' data-projects="<%= Project.company_projects(@company.id) %>"></td>
           <td><label>Due Date</label></td>
          <td id="dueDate"></td>
        </tr>

           <tr>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
          <td><label>Amount</label></td>
          <td id= "amount"> 
          </td>
        </tr>
          </tbody>
    </table>
</div>
<section id="content">
  <section class="main padder">
    <div class="row">
      <div class="col-lg-7">
        <h2><i class="icon-bar-chart text"></i>Recent Activities</h2>
      </div>
      <div class="col-lg-5 m-t-large" align="right">
        <p></p>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-12">
        <section class="panel">
          <div class="panel-body">
            <%= render :partial => "shared/flash_msg",:locals => {:obj => @workstreams} %>
            <div class="table-responsive row">
              <table class="table table-striped b-t text-small" id="user_comp_acts" data-source="<%= workstreams_url(:format => 'json', :tab_ref => "from_company", :id => @company.id) %>">
                <thead>
                  <tr>
                    <th>Recent Activities</th>
                  </tr>
                </thead>
                <tbody>
                </tbody>
              </table>
            </div>
            <footer class="row">
              <%= render "shared/report_footer"%>
            </footer>
          </div>
        </section>
      </div>
    </div>
  </section>
</section>

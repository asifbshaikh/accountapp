<div class="panel panel-info data-filter" id = "filter">
  <div class="panel-heading"><h4 class="panel-title" >GST Credit Note Filter</h4></div>
    <table class="table table-striped m-b-none text-small" id="gstcreditnote-filter">
      <tbody>
        <tr>
          <td width='10%' class='text-right'><label>Voucher no.</label></td>
          <td id="gstcreditnotevoucherno"></td>
          <td class='text-right'><label>Date</label></td>
          <td id="gstcreditnoteDate"></td>
          <td width='10%' class='text-right'><label>Customer name</label></td>
          <td id="gstcreditnotetoacc" data-toaccounts="<%= Account.credit_note_to_accounts(@company.id) %>"></td>
        </tr>
        <tr>
          <td width='10%' class='text-right'><label>Status</label></td>
          <td id="status"></td>
          <td class='text-right'><label>Amount</label></td>
          <td id="gstcreditnoteamount"></td>
          
        </tr>
      </tbody>
    </table>
</div>  

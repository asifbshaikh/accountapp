<section id="content">
  <section class="main padder">
    <div class="row">
      <div class="col-lg-7">
        <h2><i class="icon-file text"></i>Salary Structure History</h2>
      </div>
      <div class="col-lg-5 m-t-large" align="right">
        <%= link_to raw('<i class="icon-mail-reply"> </i>'), user_path(params[:user_id]), :class => 'btn btn-white btn-lg', :title=>"Back" %>
        <!-- <a href="#" class="btn btn-white btn-lg " id="use_filter"><i class="icon-filter"></i></a> -->
      </div>
    </div>
    <p></p>
    <div class="row">
      <div class="col-lg-12">
        <section class="panel">
        <div class="panel-body">
          <%= render "shared/flash_msg" %>
          <div class="table-responsive row">
            <table class="table table-striped m-b-none text-small">
              <tbody>
                <%= render :partial=>"salary_structure_history", :collection=>@salary_structure_histories%>
              </tbody>
            </table>
          </div>
          </div>
        </section>
      </div>
    </div>
  </section>
</section>

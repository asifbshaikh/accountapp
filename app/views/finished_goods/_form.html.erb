<%= form_for(@finished_good) do |f| %>
  <%= render 'shared/form_error', :object => @finished_good %>

  <div class="field">
    <%= f.label :recieved_date %><br />
    <%= f.date_select :recieved_date,:autofocus =>"autofocus" %>
  </div>
  <div class="field">
    <%= f.label :quantity %><br />
    <%= f.text_field :quantity %>
  </div>
  <div class="field">
    <%= f.label :recieved_from %><br />
    <%= f.text_field :recieved_from %>
  </div>
  <div class="field">
    <%= f.label :inventory_id %><br />
    <%= f.text_field :inventory_id %>
  </div>
  <div class="field">
    <%= f.label :company_id %><br />
    <%= f.text_field :company_id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

<h1>Listing product_settings</h1>

<table>
  <tr>
    <th>Company</th>
    <th>Multilevel pricing</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_settings.each do |product_setting| %>
  <tr>
    <td><%= product_setting.company_id %></td>
    <td><%= product_setting.multilevel_pricing %></td>
    <td><%= link_to 'Show', product_setting %></td>
    <td><%= link_to 'Edit', edit_product_setting_path(product_setting) %></td>
    <td><%= link_to 'Destroy', product_setting, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>


<section class="panel">
            <div class="panel-body bg-lighter"> 
                <div class="table-responsive col-lg-10">
              <table class="table text-small">
                <thead>
                  <tr>
                    <th class="text-center">Total Purchase</th>
                    <th class="text-center">Taxable Amount</th>
                    <th class="text-center">CGST</th>
                    <th class="text-center">SGST</th>
                    <th class="text-center">IGST</th>
                    <th class="text-center">Total Inclusive Of Taxes</th>
                    <th class="text-center">Status</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td align="center"><%= @general_info[:count] %></td>
                    <td align="center"><%= format_currency(@general_info[:taxable_amount]) %></td>
                    <td align="center"><%= format_currency(@general_info[:total_cgst]) %></td>
                    <td align="center"><%= format_currency(@general_info[:total_sgst]) %></td>
                    <td align="center"><%= format_currency(@general_info[:total_igst] )%></td>
                    <td align="center"><%=format_currency( @general_info[:total_amount]) %></td>
                    <td class="text-center" ><%= @gstr_two.status_name %>  </td>
                  </tr>
                </tbody>
              </table>
            </div>
            </div>
         </section>   
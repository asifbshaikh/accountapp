<div id="modal-archive" class="modal fade">
	<%= form_for @account, :html=>{:class=>"form-horizontal", :remote=>true} do |f| %>
		<div class="modal-dialog">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal"><i class="icon-remove"></i></button>
					<h4 class="modal-title" id="myModalLabel">Mark Inactive</h4>
				</div>
				<div class="modal-body scroll-y">
					<div class='errorSection'></div>       
					<div class="block">
						<label class="control-label">Inactive Date</label>
						<%= f.text_field :end_date, :class=>"form-control", :style=>"width:120px;" %>
					</div>
				</div>
				<div class="modal-footer">
					<div class='save-btn'></div>
				  <button class="btn btn-sm btn-success btnsave" type='submit'  data-loading-text="Saving...">Save</button>
					<button type="button" class="btn btn-sm btn-default" data-dismiss="modal">Cancel</button>
				</div>
			</div>
		</div>
	<% end %>
</div>
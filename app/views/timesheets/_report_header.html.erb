<div class="row" >
		<div class="col-lg-12" align="left">
		<% if @user_id.blank? %>
		<b> Timesheet Report For All Employees</b><br/>
		<% else %>
		<span class="xsmall light">Timesheet Report For Employee : <b><%= User.find(@user_id).full_name%></b></span><br/>
		<% end %>
		<span class="xsmall light">For Period : <b><%= @start_date%> - <%= @end_date.to_date%></b></span>
</div><br/>
</div>

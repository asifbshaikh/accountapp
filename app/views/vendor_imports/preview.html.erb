  <section id="content">
    <section class="main padder">
      <div class="row">
        <div class="col-lg-4">
       <h2><i class="icon-file text"></i>Import Status</h2>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-12">
          <section class="panel">
            <div class="panel-body">
            <div class="row">
              <div class="col-lg-12">
                <div class = "panel">
               <div id="data" data-fileid = "<%= params[:file_id] %>"></div>
                   <div class="panel-body" align="center">
                    <%= image_tag "ajax-loade.gif", :class => "loader"%> <strong>Processing...</strong>
                  </div>
                </div>
              </div>
            </div>
              
            </div>
          </section>
        </div>
      </div>
   </section>
  </section>
<script type="text/javascript">
$(document).ready(function() {
  var file_id = $("#data").attr('data-fileid');
  $.ajax({
      type: 'GET',
      url: "/vendor_imports/preview?file_id="+file_id

    });
  
}, 5000);


</script>
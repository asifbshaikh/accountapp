<div class="panel panel-info data-filter" id = "filter">
  <div class="panel-heading"><h4 class="panel-title" >Transfer Cash Filter</h4></div>
    <table class="table table-striped m-b-none text-small" id="transfercash-filter">
      <tbody>
        <tr>
          <td width='10%' class='text-right'><label>Voucher no.</label></td>
          <td id="transfer_cashvoucherno"></td>
          <td class='text-right'><label>Date</label></td>
          <td id="cashtransactionDate"></td>
          <td width='10%' class='text-right'><label>From account</label></td>
          <td id="transfer_cashfromacc" data-transfromaccounts="<%= Account.transfer_cash_from_accounts(@company.id) %>"></td>
        </tr>
        <tr>
          <td width='10%' class='text-right'><label>To account</label></td>
          <td id="transfer_cashtoacc" data-transtoaccounts="<%= Account.transfer_cash_to_accounts(@company.id) %>"></td>
          <td class='text-right'><label>Amount</label></td>
          <td id="transfer_cashamount"></td>
          <td colspan="2"></td>
        </tr>
      </tbody>
    </table>
</div>  

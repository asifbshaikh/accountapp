   <!-- Page title -->
    <%= render "shared/page_title"%>
  <!-- End of Page title -->

<!-- Page content -->
<div id="page">
   <!-- Wrapper -->
   <div class="wrapper">
     
    <!-- Left column/section -->
      <section class="column width6 first">&nbsp;
        
       <div id="tabs-all">
            <div class="colgroup">
                <div class="width3 column first"> </div>
            </div>
            <div class="clearfix"></div>
    
          <div id="payhead_type">
                <% if @payhead_types.blank? %>
                   <p class="no-tasks"><%= render :partial => "shared/no_record" %></p>
                <% else %>
                   
                        <table class="display stylized" id="example">
                          <thead>   
                            <%= render :partial => "header" %>
                          </thead>
                          <tbody>
                             <%= render :partial => "payhead_type", :collection => @payhead_types %>
                          </tbody>
                          <tfoot>
                            <%= render :partial => "footer" %>
                          </tfoot>
                        </table>
                        <div class="clear">
				          	 <div class="width3 column">
				               <%= paginate @payhead_types %>
				             </div>
				        </div> 
                <% end %>
             </div>
             <div class="clear">&nbsp;</div>
        </div>
     
   
        </div>  
     </section>
    <!-- End of Left column/section -->

    <!-- Right column/section -->
    <aside class="column width2">
   
     <div class="content-box" >
      <header>
         <h3> New Payhead Type</h3>
      </header>
        <section>
         <%= render 'form' %>
        </section>
     </div>
  
    </aside>
  <!-- End of Right column/section -->
 
   </div>
   <!-- End of Wrapper -->

</div>
<!-- End of Page content -->



